---
import faqData from "../../data/faq.json";

const schema = {
  "@context": "https://schema.org",
  "@type": "FAQPage",
  "mainEntity": faqData.map((item) => ({
    "@type": "Question",
    "name": item.question,
    "acceptedAnswer": {
      "@type": "Answer",
      "text": item.answer,
    },
  }))
};
---

<script type="application/ld+json" set:html={JSON.stringify(schema)} />
